{
  "name": "gracenook",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "worker:dev": "wrangler dev src/worker.js --local",
    "deploy:worker": "wrangler deploy src/worker.js",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=fb-clone",
    "db:migrate": "wrangler d1 execute gracenook-db --remote --file=./migrations/001_schema.sql && wrangler d1 execute gracenook-db --remote --file=./migrations/002_seed.sql"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.2",
    "wrangler": "^3.17.1"
  }
}
